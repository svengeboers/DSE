function [Cl, Cd] = liftdrag(alpha)

%     Cl = 2*pi*sin(alpha)*1.1;
%     Cd = 0.023;

    filename = 'NACA0012_RE34e4.txt';
    delimiterIn = ' ';
    headerlinesIn = 12;

    A = importdata(filename,delimiterIn, headerlinesIn);

    alphas = A.data(:,1);
    Cls = A.data(:,2);
    Cds = A.data(:,3);

    Cl = interp1(alphas,Cls,alpha);
    Cd = interp1(alphas,Cds,alpha);
    
end
